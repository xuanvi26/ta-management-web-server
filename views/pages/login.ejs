<!DOCTYPE html>
<html lang="en">
	<body class="container">
		<main>
			<div class="jumbotron">
				<h1>Login</h1>
				<form action="/login" method="POST">
					<div>
						<div class="form-group mb-2">
							<input
								type="text"
								name="username"
								id="username"
							/>
						</div>
					</div>
					<div>
						<input
							type="text"
							name="password"
              				id="password"
						/>
					</div>
					<button type="submit">Login</button>
				</form>
			</div>
		</main>
	</body>
</html>
